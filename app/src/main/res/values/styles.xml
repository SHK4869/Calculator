<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/holo_purple</item>
        <item name="colorPrimaryDark">@android:color/holo_blue_light</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">@android:color/holo_blue_bright</item>
        <item name="android:colorForeground">?attr/colorAccent</item>
    </style>

    <style name="NewTest" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="android:colorBackground">@android:color/holo_purple</item>
        <item name="android:colorForeground">#FFFFFF</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
    </style>

</resources>
