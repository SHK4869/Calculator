<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.humaira.practice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/NewTest">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" /> <!-- to specify the entry point of app -->

                <category android:name="android.intent.category.LAUNCHER" /> <!-- to specify device can be launched from launcher icon -->
            </intent-filter>
        </activity>

       <!-- <service android:name=".MyService" />-->

        <activity android:name=".Message"></activity>
    </application>

</manifest>